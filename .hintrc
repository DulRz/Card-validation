{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-viewport": "off",
    "axe/language": "off",
    "axe/text-alternatives": [
      "default",
      {
        "document-title": "off"
      }
    ],
    "button-type": "off",
    "axe/name-role-value": "off",
    "axe/forms": "off",
    "no-inline-styles": "off",
    "detect-css-reflows/layout": "off",
    "detect-css-reflows/composite": "off",
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "detect-css-reflows/paint": "off",
    "axe/parsing": [
      "default",
      {
        "duplicate-id": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "max-height: fit-content"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not ios_saf <= 14.8"
  ]
}